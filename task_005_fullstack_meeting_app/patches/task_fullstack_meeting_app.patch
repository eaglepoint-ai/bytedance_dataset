+          {busy ? "Signing in..." : "Sign in"}
+        </button>
+
+        <div style={{ height: 12 }} />
+        <div className="small">
+          No account? <Link to="/register">Register</Link>
+        </div>
+      </div>
+    </div>
+  );
+}

<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="repository_after\**" />
    <Compile Remove="repository_before/**/*.cs" />
    <Compile Remove="repository_after/**/*.cs" />
    <Compile Remove="repository_before\**" />
    <Compile Remove="tests/**/*.cs" />
    <Compile Remove="tests\**" />
    <EmbeddedResource Remove="repository_after\**" />
    <EmbeddedResource Remove="repository_before\**" />
    <EmbeddedResource Remove="tests\**" />
    <None Remove="repository_after\**" />
    <None Remove="repository_before\**" />
    <None Remove="tests\**" />
  </ItemGroup>
</Project>
